{
  "name": "api-rate-limiter",
  "version": "1.0.0",
  "description": "Standalone rate limiting service for any API with Redis backend",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "nodemon --exec ts-node src/index.ts",
    "clean": "rm -rf dist",
    "prepublish": "npm run clean && npm run build",
    "test": "node test-setup.js",
    "test:setup": "echo \"Make sure Redis is running, then run 'npm run dev' in another terminal and 'npm test'\""
  },
  "keywords": [
    "rate-limiting",
    "api",
    "redis",
    "express",
    "middleware",
    "proxy",
    "throttling"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "express-rate-limit": "^8.0.1",
    "helmet": "^8.1.0",
    "ioredis": "^5.6.1",
    "morgan": "^1.10.1",
    "redis": "^5.6.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.1.0",
    "axios": "^1.11.0",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
